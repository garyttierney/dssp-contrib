	(filecon "/etc/fonts(/.*)?" any config_file_context)
	(filecon "/usr/share/fonts(/.*)?" any data_file_context)
	(filecon "/usr/share/fontconfig(/.*)?" any data_file_context)
	(filecon "/usr/share/ghostscript/fonts(/.*)?" any data_file_context))

