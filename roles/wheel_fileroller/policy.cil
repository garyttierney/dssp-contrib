(block wheel_fileroller
	(typeattribute sessbus_chat_client_subj_type_attribute)

	(blockinherit fileroller.subj_blk)

	(optional wheel_fileroller_optional_wheel_atspird
		(call wheel_atspird.sessbus_chat_client_subj_type (subj)))

	(optional wheel_fileroller_optional_wheel_caribou
		(call wheel_caribou.sessbus_chat_client_subj_type (subj)))

	(optional wheel_fileroller_optional_wheel_dconfs
		(call wheel_dconfs.sessbus_chat_client_subj_type (subj)))

	(optional wheel_fileroller_optional_wheel_firefox
		(call wheel_firefox.auto_subj_type_transition (subj)))

	(optional wheel_fileroller_optional_wheel_gsh
		(call wheel_gsh.sessbus_chat_client_subj_type (subj)))

	(optional wheel_fileroller_optional_wheel_gvfsd
		(call wheel_gvfsd.sessbus_chat_client_subj_type (subj)))

	(optional wheel_fileroller_optional_wheel_gvfsdmd
		(call wheel_gvfsdmd.sessbus_chat_client_subj_type (subj)))

	(optional wheel_fileroller_optional_wheel_gvfsdud2
		(call wheel_gvfsdud2.sessbus_chat_client_subj_type (subj)))

	(optional wheel_fileroller_optional_wheel_gvfsdgoavm
		(call wheel_gvfsdgoavm.sessbus_chat_client_subj_type (subj)))

	(optional wheel_fileroller_optional_wheel_gvfsdgoogle
		(call wheel_gvfsdgoogle.sessbus_chat_client_subj_type (subj)))

	(optional wheel_fileroller_optional_wheel_gvfsdnetwork
		(call wheel_gvfsdnetwork.sessbus_chat_client_subj_type (subj)))

	(optional wheel_fileroller_optional_wheel_gvfsdrecent
		(call wheel_gvfsdrecent.sessbus_chat_client_subj_type (subj)))

	(optional wheel_fileroller_optional_wheel_gvfsdsftp
		(call wheel_gvfsdsftp.sessbus_chat_client_subj_type (subj)))

	(optional wheel_fileroller_optional_wheel_gvfsdtrash
		(call wheel_gvfsdtrash.sessbus_chat_client_subj_type (subj)))

	(optional wheel_fileroller_optional_wheel_nautilus
		(call wheel_nautilus.sessbus_chat_client_subj_type (subj))
		(call wheel_nautilus.auto_subj_type_transition (subj)))

	(optional wheel_fileroller_optional_wheel_sessbus
		(call wheel_sessbus.daemon (subj fileroller.cmd_file)))

	(optional wheel_fileroller_sessbus_chat_client_subj_type_optional_wheel_sessbus
		(allow sessbus_chat_client_subj_type_attribute subj (dbus (send_msg)))
		(allow subj sessbus_chat_client_subj_type_attribute (dbus (send_msg)))))

