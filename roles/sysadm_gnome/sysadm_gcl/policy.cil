(block sysadm_gcl
	(typeattribute sessbus_chat_client_subj_type_attribute)

	(blockinherit gcl.subj_blk)

	(call sysadm_atspi.tmpfs_client (subj gcl.tmpfs_user_file))

	(call sysadm_dconf.client_subj_type (subj))

	(optional sysadm_gcl_optional_sysadm_gvfsd
		(call sysadm_gvfsd.sessbus_chat_client_subj_type (subj)))

	(optional sysadm_gcl_optional_sysadm_pa
		(call sysadm_pa.auto_subj_type_transition (subj)))

	(optional sysadm_gcl_optional_sysadm_sessbus
		(call sysadm_sessbus.daemon (subj gcl.cmd_file)))

	(optional sysadm_gcl_sessbus_chat_client_subj_type_optional_sysadm_sessbus
		(allow sessbus_chat_client_subj_type_attribute subj (dbus (send_msg)))
		(allow subj sessbus_chat_client_subj_type_attribute (dbus (send_msg)))))
