(block wheel_nautilus
	(typeattribute sessbus_chat_client_subj_type_attribute)

	(blockinherit nautilus.subj_blk)

	(call wheel.wheel_subj_type (subj))
	(call wheel.cmd_auto_subj_type_transition (subj))
	(call wheel.home_cmd_auto_subj_type_transition (subj))

	(call wheel_trackers.sessbus_chat_client_subj_type (subj))

	(optional wheel_nautilus_optional_wheel_atspird
		(call wheel_atspird.sessbus_chat_client_subj_type (subj)))

	(optional wheel_nautilus_optional_wheel_dconfs
		(call wheel_dconfs.sessbus_chat_client_subj_type (subj)))

	(optional wheel_nautilus_optional_wheel_gs
		(call wheel_gs.daemon (subj nautilus.cmd_file))
		(call wheel_gs.sessbus_chat_client_subj_type (subj)))

	(optional wheel_nautilus_optional_wheel_gsh
		(call wheel_gsh.sessbus_chat_client_subj_type (subj)))

	(optional wheel_nautilus_optional_wheel_gvfsd
		(call wheel_gvfsd.sessbus_chat_client_subj_type (subj)))

	(optional wheel_nautilus_optional_wheel_gvfsdmd
		(call wheel_gvfsdmd.sessbus_chat_client_subj_type (subj)))

	(optional wheel_nautilus_optional_wheel_gvfsdud2
		(call wheel_gvfsdud2.sessbus_chat_client_subj_type (subj)))

	(optional wheel_nautilus_optional_wheel_gvfsdgoogle
		(call wheel_gvfsdgoogle.sessbus_chat_client_subj_type (subj)))

	(optional wheel_nautilus_optional_wheel_gvfsdnetwork
		(call wheel_gvfsdnetwork.sessbus_chat_client_subj_type (subj)))

	(optional wheel_nautilus_optional_wheel_gvfsdrecent
		(call wheel_gvfsdrecent.sessbus_chat_client_subj_type (subj)))

	(optional wheel_nautilus_optional_wheel_gvfsdsftp
		(call wheel_gvfsdsftp.sessbus_chat_client_subj_type (subj)))

	(optional wheel_nautilus_optional_wheel_gvfsdtrash
		(call wheel_gvfsdtrash.sessbus_chat_client_subj_type (subj)))

	(optional wheel_nautilus_optional_wheel_gvid
		(call wheel_gvid.sessbus_chat_client_subj_type (subj)))

	(optional wheel_nautilus_optional_wheel_sessbus
		(call wheel_sessbus.daemon (subj nautilus.cmd_file)))

	(optional wheel_nautilus_sessbus_chat_client_subj_type_optional_wheel_sessbus
		(allow sessbus_chat_client_subj_type_attribute subj (dbus (send_msg)))
		(allow subj sessbus_chat_client_subj_type_attribute (dbus (send_msg)))))

(block wheel_nautilusas
	(blockinherit nautilusas.subj_blk))

(block wheel_nautiluscm
	(blockinherit nautiluscm.subj_blk))
