(context livestreamer_cmd (sys.id sys.role livestreamer.cmd_file (systemlow systemlow)))

(filecon "/usr/bin/livestreamer" file livestreamer_cmd)

(filecon "HOME_DIR/\.cache/livestreamer(/.*)?" any (sys.id sys.role livestreamer.home_cache_file (systemlow systemlow)))
(filecon "HOME_DIR/\.config/livestreamer(/.*)?" any (sys.id sys.role livestreamer.home_config_file (systemlow systemlow)))
