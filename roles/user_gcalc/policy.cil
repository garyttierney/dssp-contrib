(block user_gcalc
	(typeattribute sessbus_chat_client_subj_type_attribute)

	(blockinherit gcalc.subj_blk)

	(call user_atspi.tmpfs_client (subj gcalc.tmpfs_user_file))

	(call user_dconf.client_subj_type (subj))

	(optional user_gcalc_optional_user_firefox
		(call user_firefox.auto_subj_type_transition (subj)))

	(optional user_gcalc_optional_user_gvfsd
		(call user_gvfsd.sessbus_chat_client_subj_type (subj)))

	(optional user_gcalc_optional_user_gvfsdhttp
		(call user_gvfsdhttp.rw_inherited_unix_stream_sockets (subj))
		(call user_gvfsdhttp.sessbus_chat_client_subj_type (subj)))

	(optional user_gcalc_optional_user_thunderbird
		(call user_thunderbird.auto_subj_type_transition (subj)))

	(optional user_gcalc_sessbus_chat_client_subj_type_optional_user_sessbus
		(allow sessbus_chat_client_subj_type_attribute subj (dbus (send_msg)))
		(allow subj sessbus_chat_client_subj_type_attribute (dbus (send_msg)))))

(block user_gcalcsp
	(typeattribute sessbus_chat_client_subj_type_attribute)

	(blockinherit gcalcsp.subj_blk)

	(call user_gcalc.auto_subj_type_transition (subj))

	(call user_atspi.tmpfs_client (subj gcalcsp.tmpfs_user_file))

	(optional user_gcalcsp_optional_user_sessbus
		(call user_sessbus.daemon (subj gcalcsp.cmd_file)))

	(optional user_gcalcsp_sessbus_chat_client_subj_type_optional_user_sessbus
		(allow sessbus_chat_client_subj_type_attribute subj (dbus (send_msg)))
		(allow subj sessbus_chat_client_subj_type_attribute (dbus (send_msg)))))
