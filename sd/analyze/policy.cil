	(call sd.daemon (subj cmd_file))

	(allow subj_type_attribute self (capability (net_admin dac_override kill sys_chroot sys_boot setgid setuid sys_admin sys_tty_config)))
	(allow subj_type_attribute self (process (setfscreate setsched)))
	(allow subj_type_attribute self create_udp_socket_perms)
	(allow subj_type_attribute self rw_fifo_file_perms)

	(call cmd_audit_exec_access_all_cmd_files (subj_type_attribute))
