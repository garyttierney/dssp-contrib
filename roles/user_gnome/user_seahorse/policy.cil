(block user_seahorse
	(typeattribute sessbus_chat_client_subj_type_attribute)

	(blockinherit seahorse.subj_blk)

	(call user_gnomeapp.spec_common_gnomeapplication (subj seahorse.tmpfs_user_file))

	(call user_gkrd.sessbus_chat_client_subj_type (subj))
	(call user_gkrd.stream_connect (subj))

	(call user_gpg.auto_subj_type_transition (subj))

	(call user_ssh_client.auto_subj_type_transition (subj))
	(call user_ssh_keygen.auto_subj_type_transition (subj))

	(optional user_seahorse_optional_user_gvfsdhttp
		(call user_gvfsdhttp.sessbus_chat_client_subj_type (subj)))

	(optional user_seahorse_optional_user_gvfsdnetwork
		(call user_gvfsdnetwork.sessbus_chat_client_subj_type (subj)))

	(optional user_seahorse_optional_user_gvfsdud2
		(call user_gvfsdud2.sessbus_chat_client_subj_type (subj)))

	(optional user_seahorse_optional_user_gvfsdgoavm
		(call user_gvfsdgoavm.sessbus_chat_client_subj_type (subj)))

	(optional user_seahorse_optional_user_gvfsdgoogle
		(call user_gvfsdgoogle.sessbus_chat_client_subj_type (subj)))

	(optional user_seahorse_optional_user_gvfsdrecent
		(call user_gvfsdrecent.sessbus_chat_client_subj_type (subj)))

	(optional user_seahorse_optional_user_gvfsdsftp
		(call user_gvfsdsftp.sessbus_chat_client_subj_type (subj)))

	(optional user_seahorse_optional_user_gvfsdtrash
		(call user_gvfsdtrash.sessbus_chat_client_subj_type (subj)))

	(optional user_seahorse_optional_user_sessbus
		(call user_sessbus.daemon (subj seahorse.cmd_file)))

	(optional user_seahorse_sessbus_chat_client_subj_type_optional_user_sessbus
		(allow sessbus_chat_client_subj_type_attribute subj (dbus (send_msg)))
		(allow subj sessbus_chat_client_subj_type_attribute (dbus (send_msg)))))

(block user_seahorsesap
	(typeattribute sessbus_chat_client_subj_type_attribute)

	(blockinherit seahorsesap.subj_blk)

	(call user_atspi.tmpfs_client (subj seahorsesap.tmpfs_user_file))

	(optional user_seahorsesap_optional_user_gvfsd
		(call user_gvfsd.sessbus_chat_client_subj_type (subj)))

	(optional user_seahorsesap_optional_user_sessbus
		(call user_sessbus.client_subj_type (subj)))

	(optional user_seahorsesap_sessbus_chat_client_subj_type_optional_user_sessbus
		(allow sessbus_chat_client_subj_type_attribute subj (dbus (send_msg)))
		(allow subj sessbus_chat_client_subj_type_attribute (dbus (send_msg)))))
