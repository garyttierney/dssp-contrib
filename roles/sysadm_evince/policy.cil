(block sysadm_evince
	(typeattribute sessbus_chat_client_subj_type_attribute)

	(blockinherit evince.subj_blk)

	(call sysadm_evinced.sessbus_chat_client_subj_type (subj))

	(call sysadm_atspi.tmpfs_client (subj evince.tmpfs_user_file))

	(call sysadm_dconf.client_subj_type (subj))

	(call sysadm_gvfsdmd.client_subj_type (subj))

	(optional sysadm_evince_optional_sysadm_goad
		(call sysadm_goad.sessbus_chat_client_subj_type (subj)))

	(optional sysadm_evince_optional_sysadm_gsd
		(call sysadm_gsd.sessbus_chat_client_subj_type (subj)))

	(optional sysadm_evince_optional_sysadm_gvfsd
		(call sysadm_gvfsd.sessbus_chat_client_subj_type (subj)))

	(optional sysadm_evince_optional_sysadm_gvfsdud2
		(call sysadm_gvfsdud2.sessbus_chat_client_subj_type (subj)))

	(optional sysadm_evince_optional_sysadm_gvfsdgoavm
		(call sysadm_gvfsdgoavm.sessbus_chat_client_subj_type (subj)))

	(optional sysadm_evince_optional_sysadm_gvfsdgoogle
		(call sysadm_gvfsdgoogle.sessbus_chat_client_subj_type (subj)))

	(optional sysadm_evince_optional_sysadm_gvfsdsftp
		(call sysadm_gvfsdsftp.sessbus_chat_client_subj_type (subj)))

	(optional sysadm_evince_optional_sysadm_gvfsdtrash
		(call sysadm_gvfsdtrash.sessbus_chat_client_subj_type (subj)))

	(optional sysadm_evince_optional_sysadm_gvfsdnetwork
		(call sysadm_gvfsdnetwork.sessbus_chat_client_subj_type (subj)))

	(optional sysadm_evince_optional_sysadm_nautilus
		(call sysadm_nautilus.auto_subj_type_transition (subj))
		(call sysadm_nautilus.sessbus_chat_client_subj_type (subj)))

	(optional sysadm_evince_optional_sysadm_thunderbird
		(call sysadm_thunderbird.auto_subj_type_transition (subj)))

	(optional sysadm_evince_sessbus_chat_client_subj_type_optional_sysadm_sessbus
		(allow sessbus_chat_client_subj_type_attribute subj (dbus (send_msg)))
		(allow subj sessbus_chat_client_subj_type_attribute (dbus (send_msg)))))

