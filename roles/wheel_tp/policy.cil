(block wheel_tpmct
	(blockinherit tpmct.subj_blk))

(block wheel_tpmc
	(typeattribute sessbus_chat_client_subj_type_attribute)

	(blockinherit tpmc.subj_blk)

	(optional wheel_tpmc_optional_wheel_sessbus
		(call wheel_sessbus.daemon (subj tpmc.cmd_file)))

	(optional wheel_tpmc_sessbus_chat_client_subj_type_optional_wheel_sessbus
		(allow sessbus_chat_client_subj_type_attribute subj (dbus (send_msg)))
		(allow subj sessbus_chat_client_subj_type_attribute (dbus (send_msg)))))

