(context sd_autorelabel_boot_flag (sys.id sys.role sd_autorelabel.boot_flag_file (systemlow systemlow)))
(context sd_autorelabel_cmd (sys.id sys.role sd_autorelabel.cmd_file (systemlow systemlow)))
(context sd_autorelabel_unit (sys.id sys.role sd_autorelabel.unit_file (systemlow systemlow)))

(filecon "/\.autorelabel" file sd_autorelabel_boot_flag)
(filecon "/usr/lib/systemd/fedora-autorelabel" file sd_autorelabel_cmd)
(filecon "/usr/lib/systemd/system/[^/]*fedora-autorelabel.*" file sd_autorelabel_unit)
