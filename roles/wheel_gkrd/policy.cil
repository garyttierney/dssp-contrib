(block wheel_gkrd
	(typeattribute sessbus_chat_client_subj_type_attribute)

	(blockinherit gkrd.subj_blk)

	(call wheel_gs.daemon (subj gkrd.cmd_file))

	(call wheel_gs.sessbus_chat_client_subj_type (subj))

	(optional wheel_gkrd_optional_wheel_sessbus
		(call wheel_sessbus.service_subj_type (subj)))

	(optional wheel_gkrd_sessbus_chat_client_subj_type_optional_wheel_sessbus
		(allow sessbus_chat_client_subj_type_attribute subj (dbus (send_msg)))
		(allow subj sessbus_chat_client_subj_type_attribute (dbus (send_msg)))))
