(in sd_askpwd
	(filecon "/usr/bin/systemd-ask-password" file cmd_file_context)
	(filecon "/var/run/systemd/ask-password(/.*)?" any runtime_file_context))

(in sd_askpwd_agent
	(filecon "/usr/bin/systemd-tty-ask-password-agent" file cmd_file_context)
	(filecon "/usr/lib/systemd/system/[^/]*systemd-ask-password.*" file unit_file_context)
	(filecon "/var/run/systemd/ask-password-block(/.*)?" any runtime_file_context))
