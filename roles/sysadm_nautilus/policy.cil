(block sysadm_nautilus
	(typeattribute sessbus_chat_client_subj_type_attribute)

	(blockinherit nautilus.subj_blk)

	(call sysadm.sysadm_subj_type (subj))
	(call sysadm.cmd_auto_subj_type_transition (subj))
	(call sysadm.home_cmd_auto_subj_type_transition (subj))

	(call sysadm_trackers.sessbus_chat_client_subj_type (subj))

	(optional sysadm_nautilus_optional_sysadm_atspird
		(call sysadm_atspird.sessbus_chat_client_subj_type (subj)))

	(optional sysadm_nautilus_optional_sysadm_dconfs
		(call sysadm_dconfs.sessbus_chat_client_subj_type (subj)))

	(optional sysadm_nautilus_optional_sysadm_gs
		(call sysadm_gs.daemon (subj nautilus.cmd_file))
		(call sysadm_gs.sessbus_chat_client_subj_type (subj)))

	(optional sysadm_nautilus_optional_sysadm_gsh
		(call sysadm_gsh.sessbus_chat_client_subj_type (subj)))

	(optional sysadm_nautilus_optional_sysadm_gvfsd
		(call sysadm_gvfsd.sessbus_chat_client_subj_type (subj)))

	(optional sysadm_nautilus_optional_sysadm_gvfsdmd
		(call sysadm_gvfsdmd.sessbus_chat_client_subj_type (subj)))

	(optional sysadm_nautilus_optional_sysadm_gvfsdud2
		(call sysadm_gvfsdud2.sessbus_chat_client_subj_type (subj)))

	(optional sysadm_nautilus_optional_sysadm_gvfsdnetwork
		(call sysadm_gvfsdnetwork.sessbus_chat_client_subj_type (subj)))

	(optional sysadm_nautilus_optional_sysadm_gvfsdrecent
		(call sysadm_gvfsdrecent.sessbus_chat_client_subj_type (subj)))

	(optional sysadm_nautilus_optional_sysadm_gvfsdsftp
		(call sysadm_gvfsdsftp.sessbus_chat_client_subj_type (subj)))

	(optional sysadm_nautilus_optional_sysadm_gvfsdtrash
		(call sysadm_gvfsdtrash.sessbus_chat_client_subj_type (subj)))

	(optional sysadm_nautilus_optional_sysadm_gvid
		(call sysadm_gvid.sessbus_chat_client_subj_type (subj)))

	(optional sysadm_nautilus_optional_sysadm_sessbus
		(call sysadm_sessbus.daemon (subj nautilus.cmd_file)))

	(optional sysadm_nautilus_sessbus_chat_client_subj_type_optional_sysadm_sessbus
		(allow sessbus_chat_client_subj_type_attribute subj (dbus (send_msg)))
		(allow subj sessbus_chat_client_subj_type_attribute (dbus (send_msg)))))

(block sysadm_nautilusas
	(blockinherit nautilusas.subj_blk))

(block sysadm_nautiluscm
	(blockinherit nautiluscm.subj_blk))
