;;;;;;;;;;;;;;;;;;;;;;;;;;
;
; Policy
;

(block wheel_mumble
	(blockinherit mumble.subj_blk)

	(optional wheel_mumble_optional_wheel_firefox
		(call wheel_firefox.auto_subj_type_transition (subj)))

	(optional wheel_mumble_optional_wheel_gs
		(call wheel_gs.stream_connect (subj)))

	(optional wheel_mumble_optional_wheel_pa
		(call wheel_pa.auto_subj_type_transition (subj)))

	(optional wheel_mumble_optional_wheel_sessbus
		(call wheel_sessbus.client_subj_type (subj))))
