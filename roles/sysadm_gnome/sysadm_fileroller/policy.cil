(block sysadm_fileroller
	(typeattribute sessbus_chat_client_subj_type_attribute)

	(blockinherit fileroller.subj_blk)

	(call sysadm_gnomeapp.spec_common_gnomeapplication (subj fileroller.tmpfs_user_file))
	(call sysadm_gnomeapp.file_chooser_dialog_subj_type (subj))

	(call sysadm_gvfsdmd.client_subj_type (subj))

	(optional sysadm_fileroller_optional_sysadm_sessbus
		(call sysadm_sessbus.daemon (subj fileroller.cmd_file)))

	(optional sysadm_fileroller_optional_sysadm_trackers
		(call sysadm_trackers.sessbus_chat_client_subj_type (subj)))

	(optional sysadm_fileroller_sessbus_chat_client_subj_type_optional_sysadm_sessbus
		(allow sessbus_chat_client_subj_type_attribute subj (dbus (send_msg)))
		(allow subj sessbus_chat_client_subj_type_attribute (dbus (send_msg)))))

