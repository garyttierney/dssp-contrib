(block user_dleynar
	(typeattribute sessbus_chat_client_subj_type_attribute)

	(blockinherit dleynar.subj_blk)

	(optional user_dleynar_optional_user_gvfsd
		(call user_gvfsd.sessbus_chat_client_subj_type (subj)))

	(optional user_dleynar_optional_user_sessbus
		(call user_sessbus.daemon (subj dleynar.cmd_file)))

	(optional user_dleynar_sessbus_chat_client_subj_type_optional_user_sessbus
		(allow sessbus_chat_client_subj_type_attribute subj (dbus (send_msg)))
		(allow subj sessbus_chat_client_subj_type_attribute (dbus (send_msg)))))
