(typeattribute pactl_subj_type)

(block pactl
	(blockinherit app.application_cmd_obj_blk)

	(call subj_entry (pactl_subj_type cmd_file)))

(call miscfile_read_locale (pactl_subj_type))

(call pa.mmap_tmpfs_user_files (pactl_subj_type))
(call pa.tmpfs_obj_type_transition_tmpfs_files (pactl_subj_type))
(call pa.client_subj_type (pactl_subj_type))
(call pa.read_state_all_subj (pactl_subj_type))

(optional pactl_subj_type_optional_xserver
	(call xserver_client_subj_type (pactl_subj_type)))
