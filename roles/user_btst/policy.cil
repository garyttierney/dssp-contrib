(block user_btst
	(blockinherit btst.subj_blk)

	(call user_atspi.tmpfs_client (subj btst.tmpfs_user_file))

	(optional user_btst_optional_user_dconfs
		(call user_dconfs.sessbus_chat_client_subj_type (subj)))

	(optional user_btst_optional_user_obexd
		(call user_obexd.sessbus_chat_client_subj_type (subj))))
