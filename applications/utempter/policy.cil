(roleattribute utempter_role)

(block utempter
	(blockinherit app_blk)

	(roleattributeset utempter_role sys.role)

	(roletype utempter_role subj)

	(call dev_rw_inherited_ptmx (subj))

	(call fs_search_devpts (subj))

	(call miscfile_rw_utmp_runtime_files (subj))
	(call miscfile_write_wtmp_var_log_files (subj))

	(call nss_client_subj_type (subj))

	(optional utempter_optional_myqemu
		(call myqemu_use_all_inherited_virt_ptys (subj)))

	(optional utempter_optional_screen
		(call screen_rw_inherited_unix_stream_sockets_all_subj (subj))
		(call screen_read_inherited_home_files (subj))))
