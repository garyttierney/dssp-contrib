(macro dmesg_exec ((type ARG1))
	(call can_exec (ARG1 dmesg.cmd_file)))

(macro dmesg_auto_subj_type_transition ((type ARG1))
	(call dmesg_send_signal (ARG1))
	(call auto_subj_type_transition_pattern (ARG1 dmesg.cmd_file dmesg.subj)))

(macro dmesg_send_signal ((type ARG1))
	(allow ARG1 dmesg.subj (process (signal))))

(macro dmesg_run ((type ARG1)(role ARG2))
	(call dmesg_auto_subj_type_transition (ARG1))
	(roleattributeset dmesg_role (ARG2)))
