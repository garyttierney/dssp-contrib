(block sysadm_gcalc
	(typeattribute sessbus_chat_client_subj_type_attribute)

	(blockinherit gcalc.subj_blk)

	(optional sysadm_gcalc_optional_sysadm_atspird
		(call sysadm_atspird.sessbus_chat_client_subj_type (subj)))

	(optional sysadm_gcalc_optional_sysadm_dconfs
		(call sysadm_dconfs.sessbus_chat_client_subj_type (subj)))

	(optional sysadm_gedit_optional_sysadm_firefox
		(call sysadm_firefox.auto_subj_type_transition (subj)))

	(optional sysadm_gcalc_optional_sysadm_gsh
		(call sysadm_gsh.sessbus_chat_client_subj_type (subj)))

	(optional sysadm_gcalc_optional_sysadm_gvfsd
		(call sysadm_gvfsd.sessbus_chat_client_subj_type (subj)))

	(optional sysadm_gcalc_optional_sysadm_gvfsdhttp
		(call sysadm_gvfsdhttp.rw_inherited_unix_stream_sockets (subj))
		(call sysadm_gvfsdhttp.sessbus_chat_client_subj_type (subj)))

	(optional sysadm_gcalc_optional_sysadm_sessbus
		(call sysadm_sessbus.client_subj_type (subj)))

	(optional sysadm_gcalc_optional_sysadm_thunderbird
		(call sysadm_thunderbird.auto_subj_type_transition (subj)))

	(optional sysadm_gcalc_sessbus_chat_client_subj_type_optional_sysadm_sessbus
		(allow sessbus_chat_client_subj_type_attribute subj (dbus (send_msg)))
		(allow subj sessbus_chat_client_subj_type_attribute (dbus (send_msg)))))

(block sysadm_gcalcsp
	(typeattribute sessbus_chat_client_subj_type_attribute)

	(blockinherit gcalcsp.subj_blk)

	(call sysadm_gcalc.auto_subj_type_transition (subj))

	(optional sysadm_gcalcsp_optional_sysadm_atspird
		(call sysadm_atspird.sessbus_chat_client_subj_type (subj)))

	(optional sysadm_gcalcsp_optional_sysadm_gsh
		(call sysadm_gsh.sessbus_chat_client_subj_type (subj)))

	(optional sysadm_gcalcsp_optional_sysadm_sessbus
		(call sysadm_sessbus.daemon (subj gcalcsp.cmd_file)))

	(optional sysadm_gcalcsp_sessbus_chat_client_subj_type_optional_sysadm_sessbus
		(allow sessbus_chat_client_subj_type_attribute subj (dbus (send_msg)))
		(allow subj sessbus_chat_client_subj_type_attribute (dbus (send_msg)))))
