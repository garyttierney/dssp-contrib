(context sd_backlight_cmd (sys.id sys.role sd_backlight.cmd_file (systemlow systemlow)))
(context sd_backlight_unit (sys.id sys.role sd_backlight.unit_file (systemlow systemlow)))
(context sd_backlight_var_lib (sys.id sys.role sd_backlight.var_lib_file (systemlow systemlow)))

(filecon "/usr/lib/systemd/systemd-backlight" file sd_backlight_cmd)
(filecon "/usr/lib/systemd/system/[^/]*systemd-backlight.*" file sd_backlight_unit)
(filecon "/var/lib/systemd/backlight(/.*)?" any sd_backlight_var_lib)
