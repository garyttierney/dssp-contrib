(block sysadm_caribou
	(typeattribute sessbus_chat_client_subj_type_attribute)

	(blockinherit caribou.subj_blk)

	(optional sysadm_caribou_optional_sysadm_atspird
		(call sysadm_atspird.sessbus_chat_client_subj_type (subj)))

	(optional sysadm_caribou_optional_sysadm_gcs
		(call sysadm_gcs.sessbus_chat_client_subj_type (subj)))

	(optional sysadm_caribou_optional_sysadm_gs
		(call sysadm_gs.send_signal (subj))
		(call sysadm_gs.daemon (subj caribou.cmd_file)))

	(optional sysadm_caribou_optional_sysadm_gsd
		(call sysadm_gsd.sessbus_chat_client_subj_type (subj)))

	(optional sysadm_caribou_optional_sysadm_gsh
		(call sysadm_gsh.sessbus_chat_client_subj_type (subj)))

	(optional sysadm_caribou_optional_sysadm_gts
		(call sysadm_gts.sessbus_chat_client_subj_type (subj)))

	(optional sysadm_caribou_optional_sysadm_sessbus
		(call sysadm_sessbus.daemon (subj caribou.cmd_file)))

	(optional sysadm_caribou_sessbus_chat_client_subj_type_optional_sysadm_sessbus
		(allow sessbus_chat_client_subj_type_attribute subj (dbus (send_msg)))
		(allow subj sessbus_chat_client_subj_type_attribute (dbus (send_msg)))))

(block sysadm_cariboup
	(blockinherit cariboup.subj_blk))
