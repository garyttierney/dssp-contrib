(block wheel_tracker
	(blockinherit tracker.subj_blk))

(block wheel_trackerm
	(typeattribute sessbus_chat_client_subj_type_attribute)

	(blockinherit trackerm.subj_blk)

	(call wheel_trackers.sessbus_chat_client_subj_type (subj))

	(optional wheel_trackere_optional_wheel_gvfsd
		(call wheel_gvfsd.sessbus_chat_client_subj_type (subj)))

	(optional wheel_trackere_optional_wheel_gvfsdud2
		(call wheel_gvfsdud2.sessbus_chat_client_subj_type (subj)))

	(optional wheel_trackere_optional_wheel_gvfsdmd
		(call wheel_gvfsdmd.sessbus_chat_client_subj_type (subj)))

	(optional wheel_trackere_optional_wheel_gs
		(call wheel_gs.daemon (subj trackerm.cmd_file)))

	(optional wheel_trackere_optional_wheel_gstps
		(call wheel_gstps.auto_subj_type_transition (subj)))

	(optional wheel_trackere_optional_wheel_sessbus
		(call wheel_sessbus.daemon (subj trackerm.cmd_file)))

	(optional wheel_trackere_sessbus_chat_client_subj_type_optional_wheel_sessbus
		(allow sessbus_chat_client_subj_type_attribute subj (dbus (send_msg)))
		(allow subj sessbus_chat_client_subj_type_attribute (dbus (send_msg)))))

(block wheel_trackers
	(typeattribute sessbus_chat_client_subj_type_attribute)

	(blockinherit trackers.subj_blk)

	(call wheel_trackerm.rw_inherited_fifo_files (subj))

	(optional wheel_trackers_optional_wheel_gmusic
		(call wheel_gmusic.rw_inherited_fifo_files (subj)))

	(optional wheel_trackers_optional_wheel_gvfsd
		(call wheel_gvfsd.sessbus_chat_client_subj_type (subj)))

	(optional wheel_trackers_optional_wheel_gs
		(call wheel_gs.daemon (subj trackers.cmd_file)))

	(optional wheel_trackers_optional_wheel_sessbus
		(call wheel_sessbus.daemon (subj trackers.cmd_file)))

	(optional wheel_trackers_sessbus_chat_client_subj_type_optional_wheel_sessbus
		(allow sessbus_chat_client_subj_type_attribute subj (dbus (send_msg)))
		(allow subj sessbus_chat_client_subj_type_attribute (dbus (send_msg)))))
