(tunableif enable_rbacsep
	(true
		(context xlock_cmd (sys.id sys.role xlock.cmd_file (systemlow systemlow)))
		(context xlock_home (sys.id sys.role xlock.home_user_file (systemlow systemlow))))
	(false
		(context xlock_cmd (sys.id object_r xlock.cmd_file (systemlow systemlow)))
		(context xlock_home (sys.id object_r xlock.home_user_file (systemlow systemlow)))))

(filecon "/usr/bin/xlock" file xlock_cmd)
(filecon "HOME_DIR/\.xlockrc" file xlock_home)
