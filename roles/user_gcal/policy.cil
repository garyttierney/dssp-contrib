(block user_gcal
	(typeattribute sessbus_chat_client_subj_type_attribute)

	(blockinherit gcal.subj_blk)

	(optional user_gedit_optional_user_sessbus
		(call user_sessbus.daemon (subj gcal.cmd_file)))

	(optional user_gcal_sessbus_chat_client_subj_type_optional_user_sessbus
		(allow sessbus_chat_client_subj_type_attribute subj (dbus (send_msg)))
		(allow subj sessbus_chat_client_subj_type_attribute (dbus (send_msg)))))
