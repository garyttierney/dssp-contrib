(block user_rygel
	(typeattribute sessbus_chat_client_subj_type_attribute)

	(blockinherit rygel.subj_blk)

	(optional user_rygel_optional_user_gs
		(call user_gs.daemon (subj rygel.cmd_file)))

	(optional user_rygel_optional_user_sessbus
		(call user_sessbus.daemon (subj rygel.cmd_file)))

	(optional user_rygel_sessbus_chat_client_subj_type_optional_user_sessbus
		(allow sessbus_chat_client_subj_type_attribute subj (dbus (send_msg)))
		(allow subj sessbus_chat_client_subj_type_attribute (dbus (send_msg)))))

(block user_rygelp
	(blockinherit rygelp.subj_blk))
