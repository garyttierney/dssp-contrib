(block sysadm_seahorse
	(typeattribute sessbus_chat_client_subj_type_attribute)

	(blockinherit seahorse.subj_blk)

	(call sysadm_gkrd.sessbus_chat_client_subj_type (subj))
	(call sysadm_gkrd.stream_connect (subj))

	(call sysadm_gpg.auto_subj_type_transition (subj))

	(call sysadm_ssh_client.auto_subj_type_transition (subj))
	(call sysadm_ssh_keygen.auto_subj_type_transition (subj))

	(optional sysadm_seahorse_optional_sysadm_atspird
		(call sysadm_atspird.sessbus_chat_client_subj_type (subj)))

	(optional sysadm_seahorse_optional_sysadm_caribou
		(call sysadm_caribou.sessbus_chat_client_subj_type (subj)))

	(optional sysadm_seahorse_optional_sysadm_dconfs
		(call sysadm_dconfs.sessbus_chat_client_subj_type (subj)))

	(optional sysadm_seahorse_optional_sysadm_gvfsd
		(call sysadm_gvfsd.sessbus_chat_client_subj_type (subj)))

	(optional sysadm_seahorse_optional_sysadm_gvfsdhttp
		(call sysadm_gvfsdhttp.sessbus_chat_client_subj_type (subj)))

	(optional sysadm_seahorse_optional_sysadm_gvfsdnetwork
		(call sysadm_gvfsdnetwork.sessbus_chat_client_subj_type (subj)))

	(optional sysadm_seahorse_optional_sysadm_gvfsdud2
		(call sysadm_gvfsdud2.sessbus_chat_client_subj_type (subj)))

	(optional sysadm_seahorse_optional_sysadm_gvfsdgoavm
		(call sysadm_gvfsdgoavm.sessbus_chat_client_subj_type (subj)))

	(optional sysadm_seahorse_optional_sysadm_gvfsdgoogle
		(call sysadm_gvfsdgoogle.sessbus_chat_client_subj_type (subj)))

	(optional sysadm_seahorse_optional_sysadm_gvfsdrecent
		(call sysadm_gvfsdrecent.sessbus_chat_client_subj_type (subj)))

	(optional sysadm_seahorse_optional_sysadm_gvfsdsftp
		(call sysadm_gvfsdsftp.sessbus_chat_client_subj_type (subj)))

	(optional sysadm_seahorse_optional_sysadm_gvfsdtrash
		(call sysadm_gvfsdtrash.sessbus_chat_client_subj_type (subj)))

	(optional sysadm_seahorse_optional_sysadm_gsh
		(call sysadm_gsh.sessbus_chat_client_subj_type (subj)))

	(optional sysadm_seahorse_optional_sysadm_sessbus
		(call sysadm_sessbus.daemon (subj seahorse.cmd_file)))

	(optional sysadm_seahorse_sessbus_chat_client_subj_type_optional_sysadm_sessbus
		(allow sessbus_chat_client_subj_type_attribute subj (dbus (send_msg)))
		(allow subj sessbus_chat_client_subj_type_attribute (dbus (send_msg)))))

