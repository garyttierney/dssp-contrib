(block wheel_obexd
	(typeattribute sessbus_chat_client_subj_type_attribute)

	(blockinherit obexd.subj_blk)

	(optional wheel_obexd_optional_wheel_sessbus
		(call wheel_sessbus.daemon (subj obexd.cmd_file)))

	(optional wheel_obexd_sessbus_chat_client_subj_type_optional_wheel_sessbus
		(allow sessbus_chat_client_subj_type_attribute subj (dbus (send_msg)))
		(allow subj sessbus_chat_client_subj_type_attribute (dbus (send_msg)))))
