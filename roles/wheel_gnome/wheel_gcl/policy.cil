(block wheel_gcl
	(typeattribute sessbus_chat_client_subj_type_attribute)

	(blockinherit gcl.subj_blk)

	(call wheel_gnomeapp.spec_common_gnomeapplication (subj gcl.tmpfs_user_file))

	(optional wheel_gcl_optional_wheel_pa
		(call wheel_pa.auto_subj_type_transition (subj)))

	(optional wheel_gcl_optional_wheel_sessbus
		(call wheel_sessbus.daemon (subj gcl.cmd_file)))

	(optional wheel_gcl_sessbus_chat_client_subj_type_optional_wheel_sessbus
		(allow sessbus_chat_client_subj_type_attribute subj (dbus (send_msg)))
		(allow subj sessbus_chat_client_subj_type_attribute (dbus (send_msg)))))
