(block sysadm_tracker
	(blockinherit tracker.subj_blk))

(block sysadm_trackere
	(typeattribute sessbus_chat_client_subj_type_attribute)

	(blockinherit trackere.subj_blk)

	(optional sysadm_trackere_optional_sysadm_gs
		(call sysadm_gs.daemon (subj trackere.cmd_file)))

	(optional sysadm_trackere_optional_sysadm_sessbus
		(call sysadm_sessbus.daemon (subj trackere.cmd_file)))

	(optional sysadm_trackere_sessbus_chat_client_subj_type_optional_sysadm_sessbus
		(allow sessbus_chat_client_subj_type_attribute subj (dbus (send_msg)))
		(allow subj sessbus_chat_client_subj_type_attribute (dbus (send_msg)))))

