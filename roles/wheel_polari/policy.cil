(block wheel_polari
	(typeattribute sessbus_chat_client_subj_type_attribute)

	(blockinherit polari.subj_blk)

	(call wheel_tpi.sessbus_chat_client_subj_type (subj))

	(optional wheel_polari_optional_wheel_atspird
		(call wheel_atspird.sessbus_chat_client_subj_type (subj)))

	(optional wheel_polari_optional_wheel_dconfs
		(call wheel_dconfs.sessbus_chat_client_subj_type (subj)))

	(optional wheel_polari_optional_wheel_gsh
		(call wheel_gsh.sessbus_chat_client_subj_type (subj)))

	(optional wheel_polari_optional_wheel_gvfsd
		(call wheel_gvfsd.sessbus_chat_client_subj_type (subj)))

	(optional wheel_polari_optional_wheel_tpmc
		(call wheel_tpmc.sessbus_chat_client_subj_type (subj)))

	(optional wheel_polari_optional_wheel_sessbus
		(call wheel_sessbus.daemon (subj polari.cmd_file)))

	(optional wheel_polari_sessbus_chat_client_subj_type_optional_wheel_sessbus
		(allow sessbus_chat_client_subj_type_attribute subj (dbus (send_msg)))
		(allow subj sessbus_chat_client_subj_type_attribute (dbus (send_msg)))))
