(in sduser
	(filecon "HOME_DIR/\.config/systemd(/.*)?" any home_config_file_context)
	(filecon "HOME_DIR/\.local/share/systemd(/.*)?" any home_data_file_context))

(in sduser_unit
	(filecon "/usr/lib/systemd/user(/.*)?" any unit_file_context))

(in sduser_unit_user
	(filecon "/usr/lib/systemd/system/[^/]*user@.*" file unit_file_context))
