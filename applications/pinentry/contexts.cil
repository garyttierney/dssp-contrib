(tunableif enable_rbacsep
	(true
		(context pinentry_cmd (sys.id sys.role pinentry.cmd_file (systemlow systemlow))))
	(false
		(context pinentry_cmd (sys.id object_r pinentry.cmd_file (systemlow systemlow)))))

(filecon "/usr/bin/pinentry" file pinentry_cmd)
(filecon "/usr/bin/pinentry-curses" file pinentry_cmd)
(filecon "/usr/bin/pinentry-gtk-2" file pinentry_cmd)
