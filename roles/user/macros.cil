(in user
	(macro user_subj_type ((type ARG1))
		(typeattributeset subj_type_attribute ARG1))

	(blockinherit subj.rw_inherited_fifo_files_blk)
	(blockinherit subj.rw_inherited_unix_stream_sockets_blk)
	(blockinherit subj.use_fd_blk)

	(macro cmd_auto_subj_type_transition ((type ARG1))
		(call cmd.auto_subj_type_transition (ARG1 subj)))

	(macro shell_auto_subj_type_transition ((type ARG1))
		(call cmd.shell_auto_subj_type_transition (ARG1 subj)))

	(macro home_cmd_auto_subj_type_transition ((type ARG1))
		(call usersubj.home_cmd_auto_subj_type_transition (ARG1 subj)))

	(macro dbus_send_auto_type_transition_subj_type ((type ARG1))
		(typeattributeset
			dbus_send_auto_type_transition_subj_type_attribute
				ARG1))

	(macro git_home_user_auto_type_transition_subj_type ((type ARG1))
		(typeattributeset
			git_home_user_auto_type_transition_subj_type_attribute
				ARG1))

	(macro xinit_home_user_auto_type_transition_subj_type ((type ARG1))
		(typeattributeset
			xinit_home_user_auto_type_transition_subj_type_attribute
				ARG1)))
