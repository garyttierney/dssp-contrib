(block wheel_corebird
	(typeattribute sessbus_chat_client_subj_type_attribute)

	(blockinherit corebird.subj_blk)

	(call wheel_atspi.tmpfs_client (subj corebird.tmpfs_user_file))

	(optional wheel_corebird_optional_wheel_dconfs
		(call wheel_dconfs.sessbus_chat_client_subj_type (subj)))

	(optional wheel_corebird_optional_wheel_firefox
                (call wheel_firefox.auto_subj_type_transition (subj)))

	(optional wheel_corebird_optional_wheel_gstreamer
                (call wheel_gstreamer.client_subj_type (subj)))

	(optional wheel_corebird_optional_wheel_gvfsd
		(call wheel_gvfsd.sessbus_chat_client_subj_type (subj)))

	(optional wheel_corebird_optional_wheel_gvfsdud2
		(call wheel_gvfsdud2.sessbus_chat_client_subj_type (subj)))

	(optional wheel_corebird_optional_wheel_gvfsdgoavm
		(call wheel_gvfsdgoavm.sessbus_chat_client_subj_type (subj)))

	(optional wheel_corebird_optional_wheel_gvfsdrecent
		(call wheel_gvfsdrecent.sessbus_chat_client_subj_type (subj)))

	(optional wheel_corebird_optional_wheel_gvfsdtrash
		(call wheel_gvfsdtrash.sessbus_chat_client_subj_type (subj)))

	(optional wheel_corebird_optional_wheel_gvfsdnetwork
		(call wheel_gvfsdnetwork.sessbus_chat_client_subj_type (subj)))

	(optional wheel_corebird_optional_wheel_sessbus
		(call wheel_sessbus.daemon (subj corebird.cmd_file)))

	(optional wheel_corebird_optional_wheel_nautilus
		(call wheel_nautilus.sessbus_chat_client_subj_type (subj))
                (call wheel_nautilus.auto_subj_type_transition (subj)))

	(optional wheel_corebird_optional_wheel_thunderbird
                (call wheel_thunderbird.auto_subj_type_transition (subj)))

	(optional wheel_corebird_sessbus_chat_client_subj_type_optional_wheel_sessbus
		(allow sessbus_chat_client_subj_type_attribute subj (dbus (send_msg)))
		(allow subj sessbus_chat_client_subj_type_attribute (dbus (send_msg)))))
