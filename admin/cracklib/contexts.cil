(in cracklib
	(filecon "/usr/sbin/cracklib-check" file cmd_file_context)
	(filecon "/usr/sbin/cracklib-format" file cmd_file_context)
	(filecon "/usr/sbin/cracklib-packer" file cmd_file_context)
