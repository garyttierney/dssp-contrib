(block sysadm_fileroller
	(typeattribute sessbus_chat_client_subj_type_attribute)

	(blockinherit fileroller.subj_blk)

	(call sysadm_atspi.tmpfs_client (subj fileroller.tmpfs_user_file))

	(optional sysadm_fileroller_optional_sysadm_dconfs
		(call sysadm_dconfs.sessbus_chat_client_subj_type (subj)))

	(optional sysadm_fileroller_optional_sysadm_firefox
		(call sysadm_firefox.auto_subj_type_transition (subj)))

	(optional sysadm_fileroller_optional_sysadm_gvfsd
		(call sysadm_gvfsd.sessbus_chat_client_subj_type (subj)))

	(optional sysadm_fileroller_optional_sysadm_gvfsdmd
		(call sysadm_gvfsdmd.sessbus_chat_client_subj_type (subj)))

	(optional sysadm_fileroller_optional_sysadm_gvfsdud2
		(call sysadm_gvfsdud2.sessbus_chat_client_subj_type (subj)))

	(optional sysadm_fileroller_optional_sysadm_gvfsdgoavm
		(call sysadm_gvfsdgoavm.sessbus_chat_client_subj_type (subj)))

	(optional sysadm_fileroller_optional_sysadm_gvfsdgoogle
		(call sysadm_gvfsdgoogle.sessbus_chat_client_subj_type (subj)))

	(optional sysadm_fileroller_optional_sysadm_gvfsdnetwork
		(call sysadm_gvfsdnetwork.sessbus_chat_client_subj_type (subj)))

	(optional sysadm_fileroller_optional_sysadm_gvfsdrecent
		(call sysadm_gvfsdrecent.sessbus_chat_client_subj_type (subj)))

	(optional sysadm_fileroller_optional_sysadm_gvfsdsftp
		(call sysadm_gvfsdsftp.sessbus_chat_client_subj_type (subj)))

	(optional sysadm_fileroller_optional_sysadm_gvfsdtrash
		(call sysadm_gvfsdtrash.sessbus_chat_client_subj_type (subj)))

	(optional sysadm_fileroller_optional_sysadm_nautilus
		(call sysadm_nautilus.sessbus_chat_client_subj_type (subj))
		(call sysadm_nautilus.auto_subj_type_transition (subj)))

	(optional sysadm_fileroller_optional_sysadm_sessbus
		(call sysadm_sessbus.daemon (subj fileroller.cmd_file)))

	(optional sysadm_fileroller_optional_sysadm_trackers
		(call sysadm_trackers.sessbus_chat_client_subj_type (subj)))

	(optional sysadm_fileroller_sessbus_chat_client_subj_type_optional_sysadm_sessbus
		(allow sessbus_chat_client_subj_type_attribute subj (dbus (send_msg)))
		(allow subj sessbus_chat_client_subj_type_attribute (dbus (send_msg)))))

