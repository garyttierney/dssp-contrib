	(call subj_entry (pactl_subj_type cmd_file)))

(call miscfile_read_locale (pactl_subj_type))

(call pa_mmap_tmpfs_files (pactl_subj_type))
(call pa_tmpfs_obj_type_transition_tmpfs_files (pactl_subj_type))

