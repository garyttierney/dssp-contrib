(block user_tracker
	(blockinherit tracker.subj_blk))

(block user_trackerm
	(typeattribute sessbus_chat_client_subj_type_attribute)

	(blockinherit trackerm.subj_blk)

	(call user_trackers.sessbus_chat_client_subj_type (subj))

	(optional user_trackerm_optional_user_dconfs
		(call user_dconfs.sessbus_chat_client_subj_type (subj)))

	(optional user_trackerm_optional_user_gvfsd
		(call user_gvfsd.sessbus_chat_client_subj_type (subj)))

	(optional user_trackerm_optional_user_gvfsdud2
		(call user_gvfsdud2.sessbus_chat_client_subj_type (subj)))

	(optional user_trackerm_optional_user_gvfsdmd
		(call user_gvfsdmd.sessbus_chat_client_subj_type (subj)))

	(optional user_trackerm_optional_user_gs
		(call user_gs.daemon (subj trackerm.cmd_file)))

	(optional user_trackerm_optional_user_gstps
                (call user_gstps.auto_subj_type_transition (subj)))

	(optional user_trackerm_optional_user_sessbus
		(call user_sessbus.daemon (subj trackerm.cmd_file)))

	(optional user_trackerm_sessbus_chat_client_subj_type_optional_user_sessbus
		(allow sessbus_chat_client_subj_type_attribute subj (dbus (send_msg)))
		(allow subj sessbus_chat_client_subj_type_attribute (dbus (send_msg)))))

(block user_trackers
	(typeattribute sessbus_chat_client_subj_type_attribute)

	(blockinherit trackers.subj_blk)

	(call user_trackerm.rw_inherited_fifo_files (subj))

	(optional user_trackers_optional_user_gmusic
		(call user_gmusic.rw_inherited_fifo_files (subj)))

	(optional user_trackers_optional_user_gvfsd
		(call user_gvfsd.sessbus_chat_client_subj_type (subj)))

	(optional user_trackers_optional_user_gs
		(call user_gs.daemon (subj trackers.cmd_file)))

	(optional user_trackers_optional_user_sessbus
		(call user_sessbus.daemon (subj trackers.cmd_file)))

	(optional user_trackers_sessbus_chat_client_subj_type_optional_user_sessbus
		(allow sessbus_chat_client_subj_type_attribute subj (dbus (send_msg)))
		(allow subj sessbus_chat_client_subj_type_attribute (dbus (send_msg)))))
