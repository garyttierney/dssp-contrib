(block sysadm_gom
	(typeattribute sessbus_chat_client_subj_type_attribute)

	(blockinherit gom.subj_blk)

	(optional sysadm_gom_optional_sysadm_sessbus
		(call sysadm_sessbus.daemon (subj gom.cmd_file)))

	(optional sysadm_gom_sessbus_chat_client_subj_type_optional_sysadm_sessbus
		(allow sessbus_chat_client_subj_type_attribute subj (dbus (send_msg)))
		(allow subj sessbus_chat_client_subj_type_attribute (dbus (send_msg)))))
