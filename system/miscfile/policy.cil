(block cert
	(typeattribute miscfile_obj_type_attribute)

	(blockinherit file_obj_blk)

	(call miscfile_obj_type (file)))

(block factory
	(blockinherit file_data_obj_blk))

(block fonts
	(typeattribute miscfile_cache_obj_type_attribute)
	(typeattribute miscfile_config_obj_type_attribute)
	(typeattribute miscfile_data_obj_type_attribute)

	(blockinherit file_config_obj_blk)

	(call miscfile_config_obj_type (config_file))

	(blockinherit file_data_obj_blk)

	(call miscfile_data_obj_type (data_file))

	(blockinherit file_var_cache_obj_blk)

	(call miscfile_cache_obj_type (var_cache_file)))

(block fstab
	(typeattribute miscfile_sd_status_subj_type_attribute)

	(blockinherit file_obj_blk)

	(optional miscfile_fstab_sd_status_subj_type_optional_sd
        	(allow miscfile_sd_status_subj_type_attribute file (service (status)))
        	(call file_search_config (miscfile_sd_status_subj_type_attribute))))

(block hwdata
	(blockinherit file_data_obj_blk))

(block locale
	(blockinherit file_obj_blk))

(block man
	(blockinherit file_data_obj_blk))

(block modules
	(blockinherit file_obj_blk))

(block nologin
	(blockinherit file_runtime_obj_blk))

(block network
	(blockinherit file_config_obj_blk))

(block passwords
	(blockinherit file_obj_blk))

(block shadow
	(blockinherit file_auth_obj_blk)

	(typeattribute miscfile_read_subj_type_attribute)
	(typeattribute miscfile_not_read_subj_type_attribute)

	(typeattributeset miscfile_not_read_subj_type_attribute (not (miscfile_read_subj_type_attribute)))

	(neverallow miscfile_not_read_subj_type_attribute auth_file (file (read)))

	(typeattribute miscfile_write_subj_type_attribute)
	(typeattribute miscfile_not_write_subj_type_attribute)

	(typeattributeset miscfile_not_write_subj_type_attribute (not (miscfile_write_subj_type_attribute)))

	(neverallow miscfile_not_write_subj_type_attribute auth_file (file (create write)))

	(typeattribute miscfile_relabelto_subj_type_attribute)
	(typeattribute miscfile_not_relabelto_subj_type_attribute)

	(typeattributeset miscfile_not_relabelto_subj_type_attribute (not (miscfile_relabelto_subj_type_attribute)))

	(neverallow miscfile_not_relabelto_subj_type_attribute auth_file (file (relabelto))))

(block terminfo
	(typeattribute miscfile_data_obj_type_attribute)

	(blockinherit file_config_obj_blk)

	(blockinherit file_data_obj_blk)

	(call miscfile_data_obj_type (data_file)))

(block utmp
	(blockinherit file_runtime_obj_blk))

(block wtmp
	(blockinherit file_var_log_obj_blk)

	(optional miscfile_wtmp_optional_logrotate
		(call logrotate.var_log_obj_type (var_log_file))))
