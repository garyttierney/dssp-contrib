(block wheel_btst
	(blockinherit btst.subj_blk)

	(call wheel_atspi.tmpfs_client (subj btst.tmpfs_user_file))

	(optional wheel_btst_optional_wheel_dconfs
		(call wheel_dconfs.sessbus_chat_client_subj_type (subj)))

	(optional wheel_btst_optional_wheel_obexd
		(call wheel_obexd.sessbus_chat_client_subj_type (subj))))
