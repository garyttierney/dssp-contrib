(block sysadm_gsh
	(typeattribute sessbus_chat_client_subj_type_attribute)

	(blockinherit gsh.subj_blk)

	(call sysadm.sysadm_subj_type (subj))
	(call sysadm.cmd_auto_subj_type_transition (subj))
	(call sysadm.home_cmd_auto_subj_type_transition (subj))

	(optional sysadm_gsh_optional_sysadm_atspird
		(call sysadm_atspird.sessbus_chat_client_subj_type (subj)))

	(optional sysadm_gsh_optional_sysadm_btst
		(call sysadm_btst.send_sigkill (subj)))

	(optional sysadm_gsh_optional_sysadm_firefox
		(call sysadm_firefox.send_sigkill (subj)))

	(optional sysadm_gsh_optional_sysadm_gcal
		(call sysadm_gcal.send_sigkill (subj)))

	(optional sysadm_gsh_optional_sysadm_gcl
		(call sysadm_gcl.send_sigkill (subj)))

	(optional sysadm_gsh_optional_sysadm_gcon
		(call sysadm_gcon.send_sigkill (subj)))

	(optional sysadm_gsh_optional_sysadm_gcs
		(call sysadm_gcs.send_sigkill (subj)))

	(optional sysadm_gsh_optional_sysadm_gdocs
		(call sysadm_gdocs.send_sigkill (subj)))

	(optional sysadm_gsh_optional_sysadm_gedit
		(call sysadm_gedit.send_sigkill (subj)))

	(optional sysadm_gsh_optional_sysadm_gkrd
		(call sysadm_gkrd.sessbus_chat_client_subj_type (subj)))

	(optional sysadm_gsh_optional_sysadm_gmusic
		(call sysadm_gmusic.send_sigkill (subj)))

	(optional sysadm_gsh_optional_sysadm_gs
		(call sysadm_gs.daemon (subj gsh.cmd_file))
		(call sysadm_gs.sessbus_chat_client_subj_type (subj))
		(call sysadm_gs.stream_connect (subj)))

	(optional sysadm_gsh_optional_sysadm_gsd
		(call sysadm_gsd.sessbus_chat_client_subj_type (subj)))

	(optional sysadm_gsh_optional_sysadm_gts
		(call sysadm_gts.send_sigkill (subj))
		(call sysadm_gts.sessbus_chat_client_subj_type (subj)))

	(optional sysadm_gsh_optional_sysadm_gvfsd
		(call sysadm_gvfsd.sessbus_chat_client_subj_type (subj)))

	(optional sysadm_gsh_optional_sysadm_gvfsdmd
		(call sysadm_gvfsdmd.sessbus_chat_client_subj_type (subj)))

	(optional sysadm_gsh_optional_sysadm_gvfsdud2
		(call sysadm_gvfsdud2.sessbus_chat_client_subj_type (subj)))

	(optional sysadm_gsh_optional_sysadm_nautilus
		(call sysadm_nautilus.send_sigkill (subj)))

	(optional sysadm_gsh_optional_sysadm_gvid
		(call sysadm_gvid.send_sigkill (subj)))

	(optional sysadm_gsh_optional_sysadm_nmce
		(call sysadm_nmce.send_sigkill (subj)))

	(optional sysadm_gsh_optional_sysadm_pa
		(call sysadm_pa.auto_subj_type_transition (subj)))

	(optional sysadm_gsh_optional_sysadm_polari
		(call sysadm_polari.send_sigkill (subj)))

	(optional sysadm_gsh_optional_sysadm_polkit_agent
		(call polkit_agent.auto_subj_type_transition (subj)))

	(optional sysadm_gsh_optional_wheel_sessbus
		(call sysadm_sessbus.service_subj_type (subj)))

	(optional sysadm_gsh_optional_sysadm_thunderbird
		(call sysadm_thunderbird.send_sigkill (subj)))

	(optional sysadm_gsh_sessbus_chat_client_subj_type_optional_sysadm_sessbus
		(allow sessbus_chat_client_subj_type_attribute subj (dbus (send_msg)))
		(allow subj sessbus_chat_client_subj_type_attribute (dbus (send_msg)))))

