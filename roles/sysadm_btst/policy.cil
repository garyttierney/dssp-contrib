(block sysadm_btst
	(blockinherit btst.subj_blk)

	(call sysadm_atspi.tmpfs_client (subj btst.tmpfs_user_file))

	(optional sysadm_btst_optional_sysadm_dconfs
		(call sysadm_dconfs.sessbus_chat_client_subj_type (subj)))

	(optional sysadm_btst_optional_sysadm_obexd
		(call sysadm_obexd.sessbus_chat_client_subj_type (subj))))
