;;;;;;;;;;;;;;;;;;;;;;;;;;
;
; Policy
;

(block sysadm_mumble
	(blockinherit mumble.subj_blk)

	(optional sysadm_mumble_optional_sysadm_firefox
		(call sysadm_firefox.auto_subj_type_transition (subj)))

	(optional sysadm_mumble_optional_sysadm_gconfd
		(call sysadm_gconfd.sessbus_chat_client_subj_type (subj)))

	(optional sysadm_mumble_optional_sysadm_gs
		(call sysadm_gs.stream_connect (subj)))

	(optional sysadm_mumble_optional_sysadm_gsh
		(call sysadm_gsh.sessbus_chat_client_subj_type (subj)))

	(optional sysadm_mumble_optional_sysadm_gvfsd
		(call sysadm_gvfsd.sessbus_chat_client_subj_type (subj)))

	(optional sysadm_mumble_optional_sysadm_gvfsdud2
		(call sysadm_gvfsdud2.sessbus_chat_client_subj_type (subj)))

	(optional sysadm_mumble_optional_sysadm_pa
		(call sysadm_pa.auto_subj_type_transition (subj)))

	(optional sysadm_mumble_optional_sysadm_sessbus
		(call sysadm_sessbus.service_subj_type (subj)))

	(optional sysadm_mumble_optional_sysadm_sessbus_launch
		(call sysadm_sessbus_launch.auto_subj_type_transition (subj)))

	(optional sysadm_mumble_optional_sysadm_xdgopen
		(call sysadm_xdgopen.rw_inherited_unix_stream_sockets (subj))
		(call sysadm_xdgopen.auto_subj_type_transition (subj))))
