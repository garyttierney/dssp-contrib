(in gsh
	(filecon "/usr/bin/gnome-shell" file cmd_file_context)
	(filecon "HOME_DIR/\.local/share/gnome-shell(/.*)?" any home_data_file_context))

(in gshcs
	(filecon "/usr/libexec/gnome-shell-calendar-server" file cmd_file_context))

(in gshep
	(filecon "/usr/bin/gnome-shell-extension-prefs" file cmd_file_context)
	(filecon "/usr/bin/gnome-shell-extension-tool" file cmd_file_context)
	(filecon "/usr/bin/gnome-shell-perf-tool" file cmd_file_context))

(in gshhs
	(filecon "/usr/libexec/gnome-shell-hotplug-sniffer" file cmd_file_context))

(in gsh_helper
	(filecon "/usr/libexec/gnome-shell-perf-helper" file cmd_file_context)
	(filecon "/usr/libexec/gnome-shell-portal-helper" file cmd_file_context))
