(block sysadm_gvfs
	(blockinherit gvfs.subj_blk))

(block sysadm_gvfsd
	(typeattribute daemon_subj_type_attribute)
	(typeattribute sessbus_chat_client_subj_type_attribute)

	(blockinherit gvfsd.subj_blk)

	(call sysadm_gvfsdud2.sessbus_chat_client_subj_type (subj))
	(call sysadm_gvfsdhttp.sessbus_chat_client_subj_type (subj))

	(optional sysadm_gvfsd_optional_sysadm_sessbus
		(call sysadm_sessbus.daemon (subj gvfsd.cmd_file)))

	(optional sysadm_gvfsd_sessbus_chat_client_subj_type_optional_sysadm_sessbus
		(allow sessbus_chat_client_subj_type_attribute subj (unix_stream_socket (connectto)))

		(allow sessbus_chat_client_subj_type_attribute subj (dbus (send_msg)))
		(allow subj sessbus_chat_client_subj_type_attribute (dbus (send_msg))))

	(allow daemon_subj_type_attribute daemon_subj_type_attribute (unix_stream_socket (connectto))))

