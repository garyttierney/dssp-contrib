(context sd_bootchart_cmd (sys.id sys.role sd_bootchart.cmd_file (systemlow systemlow)))
(context sd_bootchart_config (sys.id sys.role sd_bootchart.config_file (systemlow systemlow)))
(context sd_bootchart_unit (sys.id sys.role sd_bootchart.unit_file (systemlow systemlow)))

(filecon "/etc/systemd/bootchart\.conf" file sd_bootchart_config)
(filecon "/etc/systemd/bootchart\.conf\.d(/.*)?" any sd_bootchart_config)
(filecon "/usr/lib/systemd/systemd-bootchart" file sd_bootchart_cmd)
(filecon "/usr/lib/systemd/system/[^/]*systemd-bootchart.*" file sd_bootchart_unit)
