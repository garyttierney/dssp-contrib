(block sysadm_corebird
	(typeattribute sessbus_chat_client_subj_type_attribute)

	(blockinherit corebird.subj_blk)

	(optional sysadm_corebird_optional_sysadm_atspird
		(call sysadm_atspird.sessbus_chat_client_subj_type (subj)))

	(optional sysadm_corebird_optional_sysadm_dconfs
		(call sysadm_dconfs.sessbus_chat_client_subj_type (subj)))

	(optional sysadm_corebird_optional_sysadm_firefox
                (call sysadm_firefox.auto_subj_type_transition (subj)))

	(optional sysadm_corebird_optional_sysadm_gstps
                (call sysadm_gstps.auto_subj_type_transition (subj)))

	(optional sysadm_corebird_optional_sysadm_gvfsd
		(call sysadm_gvfsd.sessbus_chat_client_subj_type (subj)))

	(optional sysadm_corebird_optional_sysadm_gvfsdud2
		(call sysadm_gvfsdud2.sessbus_chat_client_subj_type (subj)))

	(optional sysadm_corebird_optional_sysadm_gvfsdgoavm
		(call sysadm_gvfsdgoavm.sessbus_chat_client_subj_type (subj)))

	(optional sysadm_corebird_optional_sysadm_gvfsdrecent
		(call sysadm_gvfsdrecent.sessbus_chat_client_subj_type (subj)))

	(optional sysadm_corebird_optional_sysadm_gvfsdtrash
		(call sysadm_gvfsdtrash.sessbus_chat_client_subj_type (subj)))

	(optional sysadm_corebird_optional_sysadm_gvfsdnetwork
		(call sysadm_gvfsdnetwork.sessbus_chat_client_subj_type (subj)))

	(optional sysadm_corebird_optional_sysadm_gsh
		(call sysadm_gsh.sessbus_chat_client_subj_type (subj)))

	(optional sysadm_corebird_optional_sysadm_sessbus
		(call sysadm_sessbus.daemon (subj corebird.cmd_file)))

	(optional sysadm_corebird_optional_sysadm_nautilus
		(call sysadm_nautilus.sessbus_chat_client_subj_type (subj))
                (call sysadm_nautilus.auto_subj_type_transition (subj)))

	(optional sysadm_corebird_optional_sysadm_thunderbird
                (call sysadm_thunderbird.auto_subj_type_transition (subj)))

	(optional sysadm_corebird_sessbus_chat_client_subj_type_optional_sysadm_sessbus
		(allow sessbus_chat_client_subj_type_attribute subj (dbus (send_msg)))
		(allow subj sessbus_chat_client_subj_type_attribute (dbus (send_msg)))))
