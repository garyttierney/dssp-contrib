	(call usersubj.home_dir_obj_type_transition_home_user_files (subj_type_attribute))

	(call udev_client_subj_type (subj_type_attribute))

	(optional analyze_subj_type_optional_sysbus
		(call sysbus_client_subj_type (subj_type_attribute))))
