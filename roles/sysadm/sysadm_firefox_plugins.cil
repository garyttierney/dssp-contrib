;
; Policy
;

(block sysadm_firefox_plugins
	(blockinherit firefox_plugins.subj_blk)

	(optional sysadm_firefox_plugins_optional_sysadm_dconf
		(call sysadm_dconf.client_subj_type (subj)))

	(optional sysadm_firefox_plugins_optional_sysadm_gsh
		(call sysadm_gsh.sessbus_chat_client_subj_type (subj)))

	(optional sysadm_firefox_plugins_optional_sysadm_pa
		(call sysadm_pa.auto_subj_type_transition (subj)))

	(optional sysadm_firefox_plugins_optional_sysadm_sessbus
		(call sysadm_sessbus.client_subj_type (subj))))
