(block user_fileroller
	(typeattribute sessbus_chat_client_subj_type_attribute)

	(blockinherit fileroller.subj_blk)

	(call user_atspi.tmpfs_client (subj fileroller.tmpfs_user_file))

	(call user_dconf.client_subj_type (subj))

	(call user_gvfsdmd.client_subj_type (subj))

	(optional user_fileroller_optional_user_firefox
		(call user_firefox.auto_subj_type_transition (subj)))

	(optional user_fileroller_optional_user_gvfsd
		(call user_gvfsd.sessbus_chat_client_subj_type (subj)))

	(optional user_fileroller_optional_user_gvfsdud2
		(call user_gvfsdud2.sessbus_chat_client_subj_type (subj)))

	(optional user_fileroller_optional_user_gvfsdgoavm
		(call user_gvfsdgoavm.sessbus_chat_client_subj_type (subj)))

	(optional user_fileroller_optional_user_gvfsdgoogle
		(call user_gvfsdgoogle.sessbus_chat_client_subj_type (subj)))

	(optional user_fileroller_optional_user_gvfsdnetwork
		(call user_gvfsdnetwork.sessbus_chat_client_subj_type (subj)))

	(optional user_fileroller_optional_user_gvfsdrecent
		(call user_gvfsdrecent.sessbus_chat_client_subj_type (subj)))

	(optional user_fileroller_optional_user_gvfsdsftp
		(call user_gvfsdsftp.sessbus_chat_client_subj_type (subj)))

	(optional user_fileroller_optional_user_gvfsdtrash
		(call user_gvfsdtrash.sessbus_chat_client_subj_type (subj)))

	(optional user_fileroller_optional_user_nautilus
		(call user_nautilus.sessbus_chat_client_subj_type (subj))
		(call user_nautilus.auto_subj_type_transition (subj)))

	(optional user_fileroller_optional_user_sessbus
		(call user_sessbus.daemon (subj fileroller.cmd_file)))

	(optional user_fileroller_optional_user_trackers
		(call user_trackers.sessbus_chat_client_subj_type (subj)))

	(optional user_fileroller_sessbus_chat_client_subj_type_optional_user_sessbus
		(allow sessbus_chat_client_subj_type_attribute subj (dbus (send_msg)))
		(allow subj sessbus_chat_client_subj_type_attribute (dbus (send_msg)))))

