;;;;;;;;;;;;;;;;;;;;;;;;;;
;
; Policy
;

(block user_xdgopen
	(blockinherit xdgopen.subj_blk)

	(optional user_xdgopen_optional_user_firefox
		(call user_firefox.auto_subj_type_transition (subj)))

	(optional user_xdgopen_optional_user_gs
		(call user_gs.forked_subj_type (subj)))

	(optional user_xdgopen_optional_user_gvfs
		(call user_gvfs.auto_subj_type_transition (subj)))

	(optional user_xdgopen_optional_user_sessbus
		(call user_sessbus.client_subj_type (subj))))
