(block wheel_seahorse
	(typeattribute sessbus_chat_client_subj_type_attribute)

	(blockinherit seahorse.subj_blk)

	(call wheel_gkrd.sessbus_chat_client_subj_type (subj))
	(call wheel_gkrd.stream_connect (subj))

	(call wheel_gpg.auto_subj_type_transition (subj))

	(call wheel_ssh_client.auto_subj_type_transition (subj))
	(call wheel_ssh_keygen.auto_subj_type_transition (subj))

	(call wheel_atspi.tmpfs_client (subj seahorse.tmpfs_user_file))

	(optional wheel_seahorse_optional_wheel_dconfs
		(call wheel_dconfs.sessbus_chat_client_subj_type (subj)))

	(optional wheel_seahorse_optional_wheel_gvfsd
		(call wheel_gvfsd.sessbus_chat_client_subj_type (subj)))

	(optional wheel_seahorse_optional_wheel_gvfsdhttp
		(call wheel_gvfsdhttp.sessbus_chat_client_subj_type (subj)))

	(optional wheel_seahorse_optional_wheel_gvfsdnetwork
		(call wheel_gvfsdnetwork.sessbus_chat_client_subj_type (subj)))

	(optional wheel_seahorse_optional_wheel_gvfsdud2
		(call wheel_gvfsdud2.sessbus_chat_client_subj_type (subj)))

	(optional wheel_seahorse_optional_wheel_gvfsdgoavm
		(call wheel_gvfsdgoavm.sessbus_chat_client_subj_type (subj)))

	(optional wheel_seahorse_optional_wheel_gvfsdgoogle
		(call wheel_gvfsdgoogle.sessbus_chat_client_subj_type (subj)))

	(optional wheel_seahorse_optional_wheel_gvfsdrecent
		(call wheel_gvfsdrecent.sessbus_chat_client_subj_type (subj)))

	(optional wheel_seahorse_optional_wheel_gvfsdsftp
		(call wheel_gvfsdsftp.sessbus_chat_client_subj_type (subj)))

	(optional wheel_seahorse_optional_wheel_gvfsdtrash
		(call wheel_gvfsdtrash.sessbus_chat_client_subj_type (subj)))

	(optional wheel_seahorse_optional_wheel_sessbus
		(call wheel_sessbus.daemon (subj seahorse.cmd_file)))

	(optional wheel_seahorse_sessbus_chat_client_subj_type_optional_wheel_sessbus
		(allow sessbus_chat_client_subj_type_attribute subj (dbus (send_msg)))
		(allow subj sessbus_chat_client_subj_type_attribute (dbus (send_msg)))))

(block wheel_seahorsesap
	(typeattribute sessbus_chat_client_subj_type_attribute)

	(blockinherit seahorsesap.subj_blk)

	(call wheel_atspi.tmpfs_client (subj seahorsesap.tmpfs_user_file))

	(optional wheel_seahorsesap_optional_wheel_gvfsd
		(call wheel_gvfsd.sessbus_chat_client_subj_type (subj)))

	(optional wheel_seahorsesap_optional_wheel_sessbus
		(call wheel_sessbus.client_subj_type (subj)))

	(optional wheel_seahorsesap_sessbus_chat_client_subj_type_optional_wheel_sessbus
		(allow sessbus_chat_client_subj_type_attribute subj (dbus (send_msg)))
		(allow subj sessbus_chat_client_subj_type_attribute (dbus (send_msg)))))
