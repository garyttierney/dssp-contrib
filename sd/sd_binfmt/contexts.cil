(context sd_binfmt_cmd (sys.id sys.role sd_binfmt.cmd_file (systemlow systemlow)))
(context sd_binfmt_config (sys.id sys.role sd_binfmt.config_file (systemlow systemlow)))
(context sd_binfmt_unit (sys.id sys.role sd_binfmt.unit_file (systemlow systemlow)))

(filecon "/etc/binfmt\.d(/.*)?" any sd_binfmt_config)
(filecon "/usr/lib/systemd/systemd-binfmt" file sd_binfmt_cmd)
(filecon "/usr/lib/systemd/system/[^/]*systemd-binfmt.*" file sd_binfmt_unit)
