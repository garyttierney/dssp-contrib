(block wheel_caribou
	(typeattribute sessbus_chat_client_subj_type_attribute)

	(blockinherit caribou.subj_blk)

	(optional wheel_caribou_optional_wheel_atspird
		(call wheel_atspird.sessbus_chat_client_subj_type (subj)))

	(optional wheel_caribou_optional_wheel_gcs
		(call wheel_gcs.sessbus_chat_client_subj_type (subj)))

	(optional wheel_caribou_optional_wheel_gs
		(call wheel_gs.daemon (subj caribou.cmd_file)))

	(optional wheel_caribou_optional_wheel_gsd
		(call wheel_gsd.sessbus_chat_client_subj_type (subj)))

	(optional wheel_caribou_optional_wheel_gsh
		(call wheel_gsh.sessbus_chat_client_subj_type (subj)))

	(optional wheel_caribou_optional_wheel_gts
		(call wheel_gts.sessbus_chat_client_subj_type (subj)))

	(optional wheel_caribou_optional_wheel_sessbus
		(call wheel_sessbus.daemon (subj caribou.cmd_file)))

	(optional wheel_caribou_sessbus_chat_client_subj_type_optional_wheel_sessbus
		(allow sessbus_chat_client_subj_type_attribute subj (dbus (send_msg)))
		(allow subj sessbus_chat_client_subj_type_attribute (dbus (send_msg)))))

(block wheel_cariboup
	(blockinherit cariboup.subj_blk))
