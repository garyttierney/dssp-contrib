(in cert
	(filecon "/etc/pki(/.*)?" any file_file_context)
	(filecon "/etc/ssl(/.*)?" any file_file_context))

(in cryptopol
	(filecon "/etc/crypto-policies(/.*)?" any config_file_context))

(in cryptopol_state
