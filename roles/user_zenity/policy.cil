(block user_zenity
	(typeattribute sessbus_chat_client_subj_type_attribute)

	(blockinherit zenity.subj_blk)

	(call user.cmd_auto_subj_type_transition (subj))
	(call user.shell_auto_subj_type_transition (subj))

	(call user.home_cmd_auto_subj_type_transition (subj))

	(optional user_zenity_optional_user_atspird
		(call user_atspird.sessbus_chat_client_subj_type (subj)))

	(optional user_zenity_optional_user_dconfs
		(call user_dconfs.sessbus_chat_client_subj_type (subj)))

	(optional user_zenity_optional_user_gvfsd
		(call user_gvfsd.sessbus_chat_client_subj_type (subj)))

	(optional user_zenity_optional_user_gvfsdud2
		(call user_gvfsdud2.sessbus_chat_client_subj_type (subj)))

	(optional user_zenity_optional_user_gvfsdgoavm
		(call user_gvfsdgoavm.sessbus_chat_client_subj_type (subj)))

	(optional user_zenity_optional_user_gvfsdhttp
		(call user_gvfsdhttp.sessbus_chat_client_subj_type (subj)))

	(optional user_zenity_optional_user_gvfsdnetwork
		(call user_gvfsdnetwork.sessbus_chat_client_subj_type (subj)))

	(optional user_zenity_optional_user_gvfsdsftp
		(call user_gvfsdsftp.sessbus_chat_client_subj_type (subj)))

	(optional user_zenity_optional_user_gvfsdtrash
		(call user_gvfsdtrash.sessbus_chat_client_subj_type (subj)))

	(optional user_zenity_optional_user_gsh
		(call user_gsh.sessbus_chat_client_subj_type (subj)))

	(optional user_zenity_optional_user_nautilus
		(call user_nautilus.sessbus_chat_client_subj_type (subj))
		(call user_nautilus.auto_subj_type_transition (subj)))

	(optional user_zenity_optional_user_sessbus
		(call user_sessbus.client_subj_type (subj)))

	(optional user_zenity_sessbus_chat_client_subj_type_optional_user_sessbus
		(allow sessbus_chat_client_subj_type_attribute subj (dbus (send_msg)))
		(allow subj sessbus_chat_client_subj_type_attribute (dbus (send_msg)))))
