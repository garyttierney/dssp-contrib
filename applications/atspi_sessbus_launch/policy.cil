(typeattribute atspi_sessbus_launch_subj_type)
(typeattribute atspi_sessbus_launch_sessbus_chat_client_subj_type)

(block atspi_sessbus_launch
	(blockinherit app_cmd_obj_blk)

	(call subj_entry (atspi_sessbus_launch_subj_type cmd_file)))

(allow atspi_sessbus_launch_subj_type self rw_fifo_file_perms)

(call dev_read_urandom (atspi_sessbus_launch_subj_type))

(call file_read_data_files (atspi_sessbus_launch_subj_type))

(call nss_client_subj_type (atspi_sessbus_launch_subj_type))

(optional atspi_sessbus_launch_subj_type_optional_sessbus
        (call sessbus_acquire_service_all_subj (atspi_sessbus_launch_subj_type))
        (call sessbus_client_all_subj (atspi_sessbus_launch_subj_type)))

(optional atspi_sessbus_launch_subj_type_optional_xserver
	(call xserver_client_subj_type (atspi_sessbus_launch_subj_type)))

(optional atspi_sessbus_launch_sessbus_chat_client_subj_type_optional_sessbus
	(allow atspi_sessbus_launch_sessbus_chat_client_subj_type atspi_sessbus_launch_subj_type (dbus (send_msg)))
	(allow atspi_sessbus_launch_subj_type atspi_sessbus_launch_sessbus_chat_client_subj_type (dbus (send_msg))))
