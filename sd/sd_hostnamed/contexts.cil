(context sd_hostnamed_cmd (sys.id sys.role sd_hostnamed.cmd_file (systemlow systemlow)))
(context sd_hostnamed_unit (sys.id sys.role sd_hostnamed.unit_file (systemlow systemlow)))

(filecon "/usr/lib/systemd/systemd-hostnamed" file sd_hostnamed_cmd)
(filecon "/usr/lib/systemd/system/[^/]*systemd-hostnamed.*" file sd_hostnamed_unit)
