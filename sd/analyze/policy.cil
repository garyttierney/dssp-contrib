	(call fs_getattr_sysfs_fs (subj_type_attribute))
	(call fs_read_devtmpfs_lnk_files (subj_type_attribute))
	(call fs_read_cgroup (subj_type_attribute))
	(call fs_read_sysfs (subj_type_attribute))

	(call storage_getattr_all (subj_type_attribute))

	(call term_use_unallocated_ttys (subj_type_attribute))

	(call miscfile_fstab_sd_status_subj_type (subj_type_attribute))
	(call miscfile_read_all_terminfo (subj_type_attribute))
	(call miscfile_read_locale (subj_type_attribute))
