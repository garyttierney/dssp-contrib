(block gconf
	(typeattribute client_subj_type_attribute)

	(blockinherit file.config_obj_blk)

	(blockinherit usersubj.home_config_obj_blk)

	(call usersubj.create_home_config_dirs (client_subj_type_attribute))
	(call usersubj.home_dir_add_obj_type_transition_home_config (client_subj_type_attribute))

	(call manage_home_config (client_subj_type_attribute))
	(call home_config_obj_type_transition_home_config (client_subj_type_attribute)))

(block gconfdm
	(blockinherit subj.common_subj_blk)

	(blockinherit cmd.obj_blk)

	(call subj.entry (subj cmd_file))

	(roletype sys.role subj)

	(call gconf.read_config (subj))

	(optional gconfdm_optional_sysbus
		(call sysbus.daemon_subj_type (subj))))

(block gconfd
	(typeattribute subj_type_attribute)

	(blockinherit app.application_cmd_obj_blk)

	(call subj.entry (subj_type_attribute cmd_file)))

(block gconftool
	(typeattribute subj_type_attribute)

	(blockinherit app.application_cmd_obj_blk)

	(call subj.entry (subj_type_attribute cmd_file)))

(block gsettingsdc
	(typeattribute subj_type_attribute)

	(blockinherit app.application_cmd_obj_blk)

	(call subj.entry (subj_type_attribute cmd_file))

	(blockinherit usersubj.home_data_obj_blk)

	(call usersubj.create_home_data_dirs (subj_type_attribute))
	(call usersubj.home_dir_add_obj_type_transition_home_data (subj_type_attribute))

	(call manage_home_data_files (subj_type_attribute))
	(call home_data_obj_type_transition_home_data (subj_type_attribute)))
