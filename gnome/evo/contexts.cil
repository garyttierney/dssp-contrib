(in evo
        (filecon "HOME_DIR/\.cache/evolution(/.*)?" any home_cache_file_context)
        (filecon "HOME_DIR/\.config/evolution(/.*)?" any home_config_file_context)
        (filecon "HOME_DIR/\.local/share/evolution(/.*)?" any home_data_file_context))

(in evoaf
	(filecon "/usr/libexec/evolution-addressbook-factory" file cmd_file_context)
	(filecon "/usr/libexec/evolution-addressbook-factory-subprocesss" file cmd_file_context))

(in evocf
	(filecon "/usr/libexec/evolution-calendar-factory" file cmd_file_context)
	(filecon "/usr/libexec/evolution-calendar-factory-subprocess" file cmd_file_context))

(in evosr
	(filecon "/usr/libexec/evolution-source-registry" file cmd_file_context))

(in evo_helper
	(filecon "/usr/libexec/camel-index-control-.*" file cmd_file_context)
	(filecon "/usr/libexec/camel-lock-helper-.*" file cmd_file_context)
	(filecon "/usr/libexec/evolution-scan-gconf-tree-xml" file cmd_file_context)
	(filecon "/usr/libexec/evolution-user-prompter" file cmd_file_context))
