(tunableif enable_rbacsep
	(true
		(context gpg_agent_cmd (sys.id sys.role gpg_agent.cmd_file (systemlow systemlow)))
		(context gpg_agent_home (sys.id sys.role gpg_agent.home_user_file (systemlow systemlow))))
	(false
		(context gpg_agent_cmd (sys.id object_r gpg_agent.cmd_file (systemlow systemlow)))
		(context gpg_agent_home (sys.id object_r gpg_agent.home_user_file (systemlow systemlow)))))

(filecon "/usr/bin/gpg-agent" file gpg_agent_cmd)
(filecon "HOME_DIR/\.gnupg/gpg-agent\.conf" file gpg_agent_home)
(filecon "HOME_DIR/\.gnupg/gpg-agent\.log" file gpg_agent_home)
(filecon "HOME_DIR/\.gnupg/S\.gpg-agent" socket gpg_agent_home)
(filecon "HOME_DIR/\.gnupg/S\.gpg-agent\.ssh" socket gpg_agent_home)
(filecon "HOME_DIR/\.gnupg/sshcontrol" file gpg_agent_home)
(filecon "HOME_DIR/\.gnupg/private-keys-v1\.d(/.*)?" any gpg_agent_home)
