(block sysadm_rygel
	(typeattribute sessbus_chat_client_subj_type_attribute)

	(blockinherit rygel.subj_blk)

	(optional sysadm_rygel_optional_sysadm_sessbus
		(call sysadm_sessbus.daemon (subj rygel.cmd_file)))

	(optional sysadm_rygel_sessbus_chat_client_subj_type_optional_sysadm_sessbus
		(allow sessbus_chat_client_subj_type_attribute subj (dbus (send_msg)))
		(allow subj sessbus_chat_client_subj_type_attribute (dbus (send_msg)))))

(block sysadm_rygelp
	(blockinherit rygelp.subj_blk))
