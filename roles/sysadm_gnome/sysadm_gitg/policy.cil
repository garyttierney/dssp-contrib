(block sysadm_gitg
	(typeattribute sessbus_chat_client_subj_type_attribute)

	(blockinherit gitg.subj_blk)

	(call sysadm_gnomeapp.spec_common_gnomeapplication (subj gitg.tmpfs_user_file))
	(call sysadm_gnomeapp.file_chooser_dialog_subj_type (subj))

	(optional sysadm_gitg_optional_sysadm_gkrd
		(call sysadm_gkrd.stream_connect (subj)))

	(optional sysadm_gitg_optional_sysadm_gvfsdhttp
		(call sysadm_gvfsdhttp.rw_inherited_unix_stream_sockets (subj))
		(call sysadm_gvfsdhttp.sessbus_chat_client_subj_type (subj)))

	(optional sysadm_gitg_optional_sysadm_webkitwp
		(call sysadm_webkitwp.sessbus_chat_client_subj_type (subj))
		(call sysadm_webkitwp.auto_subj_type_transition (subj)))

	(optional sysadm_gitg_optional_sysadm_sessbus
		(call sysadm_sessbus.service_subj_type (subj)))

	(optional sysadm_gitg_sessbus_chat_client_subj_type_optional_sysadm_sessbus
		(allow sessbus_chat_client_subj_type_attribute subj (dbus (send_msg)))
		(allow subj sessbus_chat_client_subj_type_attribute (dbus (send_msg)))))
